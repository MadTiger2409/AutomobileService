{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build AutomobileWebService",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/AutomobileWebService/AutomobileWebService.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build AutomobileWebService.Tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/AutomobileWebService.Test/AutomobileWebService.Tests.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "grunt SASS",
            "command": "grunt",
            "type": "shell",
            "problemMatcher": [
                "$node-sass"
            ]
        }
    ]
}